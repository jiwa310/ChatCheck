{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "images/heart.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.discord.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
        "resources": ["images/brilliant_32x.png"],
        "matches": ["*://*.discord.com/*"]
    }
  ]
}